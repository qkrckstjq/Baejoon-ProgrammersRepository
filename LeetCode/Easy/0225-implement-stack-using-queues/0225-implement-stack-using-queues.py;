class MyStack(object):

    def __init__(self):
        self.stack = []

    def push(self, x):
        self.stack.append(x)
        """
        :type x: int
        :rtype: None
        """

    def pop(self):
        if len(self.stack) != 0:
            return self.stack.pop()
        """
        :rtype: int
        """

    def top(self):
        if len(self.stack) != 0:
            return self.stack[len(self.stack)-1]
        """
        :rtype: int
        """

    def empty(self):
        return True if len(self.stack) == 0 else False
        """
        :rtype: bool
        """

# Your MyStack object will be instantiated and called as such:
# obj = MyStack()
# obj.push(x)
# param_2 = obj.pop()
# param_3 = obj.top()
# param_4 = obj.empty()