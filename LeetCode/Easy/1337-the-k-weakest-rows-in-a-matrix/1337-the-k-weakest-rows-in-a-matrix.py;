class Solution:
    def kWeakestRows(self, mat: List[List[int]], k: int) -> List[int]:
        min_heap = []
        for i in range(len(mat)):
            heapq.heappush(min_heap, (Counter(mat[i])[1], i))

        heapq_k = heapq.nsmallest(k, min_heap)
        return list(map(lambda x: x[1],heapq_k))