class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        uniqueChar = {}
        count = 0
        max = 0
        i = 0
        startIndex = i
        while i < len(s):
            if s[i] in uniqueChar:
                i = startIndex
                startIndex = i+1
                uniqueChar = {}
                count = 0
            if not s[i] in uniqueChar:
                uniqueChar[s[i]] = True
                count += 1
                max = count if max < count else max
            i += 1

        return max